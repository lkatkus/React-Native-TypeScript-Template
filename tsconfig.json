{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "src",
    "paths": {
      "app/*": ["./app/*"],
      "i18n/*": ["./i18n/*"]
    }
  },
  "exclude": ["node_modules"]
}
